ist())\n            w, h = x2 - x1, y2 - y1\n            area = w * h\n    \n            if area < 500 or area > 20000:\n                continue  # filter tiny or giant false detections\n    \n            detections.append(([x1, y1, w, h], conf.item(), 'player'))\n            cv2.rectangle(frame, (x1, y1), (x2, y2), (0, 0, 255), 1)\n\n\n    print(f\"[Frame {frame_id}] Players detected: {len(detections)}\")\n\n    # Track players\n    tracks = tracker.update_tracks(detections, frame=frame)\n\n    for track in tracks:\n        if not track.is_confirmed():\n            continue\n        track_id = track.track_id\n        l, t, r, b = map(int, track.to_ltrb())\n        cv2.rectangle(frame, (l, t), (r, b), (0, 255, 0), 2)\n        cv2.putText(frame, f\"Player {track_id}\", (l, t - 10), cv2.FONT_HERSHEY_SIMPLEX,\n                    0.6, (0, 255, 0), 2)\n\n    out.write(frame)\n\ncap.release()\nout.release()\nprint(\"✅ Done: output_tracked.mp4 saved.\")\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-07-12T14:11:26.968911Z","iopub.execute_input":"2025-07-12T14:11:26.969246Z","iopub.status.idle":"2025-07-12T14:12:07.006910Z","shell.execute_reply.started":"2025-07-12T14:11:26.969215Z","shell.execute_reply":"2025-07-12T14:12:07.006178Z"},"collapsed":true,"jupyter":{"outputs_hidden":true}},"outputs":[{"name":"stdout","text":"\n0: 384x640 1 ball, 16 players, 2 referees, 69.8ms\nSpeed: 2.3ms preprocess, 69.8ms inference, 2.0ms postprocess per image at shape (1, 3, 384, 640)\n[Frame 1] Players detected: 16\n\n0: 384x640 18 players, 2 referees, 39.2ms\nSpeed: 1.9ms preprocess, 39.2ms inference, 1.7ms postprocess per image at shape (1, 3, 384, 640)\n[Frame 2] Players detected: 18\n\n0: 384x640 1 ball, 16 players, 2 referees, 38.2ms\nSpeed: 1.7ms preprocess, 38.2ms inference, 1.6ms postprocess per image at shape (1, 3, 384, 640)\n[Frame 3] Players detected: 16\n\n0: 384x640 1 ball, 14 players, 2 referees, 38.2ms\nSpeed: 1.9ms preprocess, 38.2ms inference, 1.9m