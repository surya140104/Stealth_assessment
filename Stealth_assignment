video path\nvideo_path = \"/kaggle/input/videoframe/15sec_input_720p.mp4\"\ncap = cv2.VideoCapture(video_path)\n\n# Output video settings\nwidth, height = int(cap.get(3)), int(cap.get(4))\nfps = int(cap.get(cv2.CAP_PROP_FPS))\nout = cv2.VideoWriter(\n    \"output_tracked_final.mp4\",\n    cv2.VideoWriter_fourcc(*'mp4v'),\n    fps,\n    (width, height)\n)\n\n# Inference loop\nwhile True:\n    ret, frame = cap.read()\n    if not ret:\n        break\n\n    # Detect objects with YOLO\n    results = model(frame, verbose=False)[0]\n    detections = []\n\n    for box in results.boxes:\n        cls = int(box.cls[0])\n        conf = float(box.conf[0])\n        if cls == 2 and conf > 0.3:  # class 2 = player\n            x1, y1, x2, y2 = map(int, box.xyxy[0])\n            detections.append(([x1, y1, x2 - x1, y2 - y1], conf))\n\n    bbox_xywh = []\n    confs = []\n\n    for det in detections:\n        x, y, w, h = det[0]\n        bbox_xywh.append([x + w / 2, y + h / 2, w, h])\n        confs.append(det[1])\n\n    # Update DeepSORT tracker\n    if bbox_xywh:\n        clss = [0] * len(bbox_xywh)  # Dummy class labels (required by DeepSort)\n    \n        outputs = deepsort.update(\n            torch.Tensor(bbox_xywh),\n            torch.Tensor(confs),\n            clss,\n            frame  # Original frame\n        )\n\n    # Draw tracked boxes\n    if outputs is not None and len(outputs) > 0:\n        for output in outputs:\n            if len(output) == 5:\n                x1, y1, x2, y2, track_id = output\n                x1, y1, x2, y2 = map(int, [x1, y1, x2, y2])\n                cv2.rectangle(frame, (x1, y1), (x2, y2), (0, 255, 0), 2)\n                cv2.putText(frame, f\"Player {track_id}\", (x1, y1 - 10),\n                            cv2.FONT_HERSHEY_SIMPLEX, 0.6, (0, 255, 0), 2)\n\n\n    out.write(frame)\n\n# Clean up\ncap.release()\nout.release()\n# cv2.destroyAllWindows()\n\nprint(\"✅ Player tracking complete! Output saved to `output_tracked_final.mp4`\")\n","metadata